numPedidos = int(input())

for i in range(1, numPedidos + 1):
    prato = input()
    calorias = int(input())
    ehVegano = False
    
    if input() == 's':
      tipoVegano = 'Vegano'
    else:
      tipoVegano = 'NÃ£o-Vegano'
    
    print(f'Pedido {i}: {prato} {tipoVegano} - {calorias} calorias')
